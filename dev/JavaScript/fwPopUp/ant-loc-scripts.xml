<?xml version="1.0"?>
<!-- Open the build project and give it a name to display. -->
<project name="fwPopUp :: 'scripts' version" default="init">
	<!-- Display a clear message referencing the current build script. -->
	<echo message="Building 'fwPopUp' to the 'scripts' folder."/>
	<!-- Define the path to the location to build to and set it to the "buildLocaction" variable. -->
	<property name="buildLocation" location="../../../public/scripts"/>
	<!-- Import the core ant build script. -->
  <import file="./ant-build.xml"/>
</project>