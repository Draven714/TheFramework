{
    "name": "mamook/framework",
    "description": "mvc style php framework",
    "type": "framework",
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "license": "mit",
    "minimum-stability": "stable",
    "require": {
        "symfony/yaml": "^3.1",
        "symfony/console": "^3.1",
        "league/route": "^2.0",
        "monolog/monolog": "^1.21",
        "doctrine/orm": "^2.5",
        "nesbot/carbon": "^1.21",
        "vlucas/phpdotenv": "^2.4",
        "ramsey/uuid": "^3.5",
        "guzzlehttp/guzzle": "~5.3|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Mamook\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mamook\\Framework\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    }
}
